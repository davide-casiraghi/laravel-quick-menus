{
    "name": "davide-casiraghi/laravel-quick-menus",
    "description": "A Laravel package to generate Bootstrap 4 multi language menus.",
    "keywords": [
        "davide-casiraghi",
        "laravel-quick-menus"
    ],
    "homepage": "https://github.com/davide-casiraghi/laravel-quick-menus",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Davide Casiraghi",
            "email": "davide.casiraghi@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.2",
        "astrotomic/laravel-translatable": "^11.1",
        "illuminate/support": "^7.0",
        "mcamara/laravel-localization": "^1.3",
        "nesbot/carbon": "^2.17"
    },
    "require-dev": {
        "orchestra/testbench": "^5.0",
        "phpunit/phpunit": "^8.5"
    },
    "autoload": {
        "psr-4": {
            "DavideCasiraghi\\LaravelQuickMenus\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "DavideCasiraghi\\LaravelQuickMenus\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "DavideCasiraghi\\LaravelQuickMenus\\LaravelQuickMenusServiceProvider"
            ],
            "aliases": {
                "LaravelQuickMenus": "DavideCasiraghi\\LaravelQuickMenus\\LaravelQuickMenus"
            }
        }
    }
}
